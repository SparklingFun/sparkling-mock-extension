{
    "name": "Sparkling Mock",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "前端数据Mock插件，可使用线上sparkling.fun的服务。",
    "icons": {
        "128": "./ext-icon.png"
    },
    "background": {
        "persistent": true,
        "page": "./background.html"
    },
    "options_page": "./options.html",
    "permissions": [
        "<all_urls>",
        "webRequest",
        "webRequestBlocking",
        "storage",
        "tabs"
    ],
    "incognito":"split",
    "browser_action": {
        "default_icon": "./ext-icon.png"
    }
}